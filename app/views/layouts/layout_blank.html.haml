%html{:itemscope => "", :itemtype => "https://schema.org/", :lang => "en-US"}
  / <![endif]
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %meta{:charset => "utf-8"}/
    %title Atlas
    = yield :before_scripts
    = render "shared/header_meta"
    / Font awesome
    %link{:href => "https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css", :rel => "stylesheet"}/
    %link{:href => "https://fonts.googleapis.com/css?family=Work+Sans:400,600", :rel => "stylesheet", :type => "text/css"}/
    = stylesheet_link_tag 'application', media: 'all'
    = javascript_include_tag 'application-blank'
    = csrf_meta_tags
    = yield :after_scripts
  %body{:class => "#{yield :body_class}"}
    .wrapper
      = yield
      = yield :after_body
    / wrapper
