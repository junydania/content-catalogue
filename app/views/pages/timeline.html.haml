.content-wrapper
  / Content Header (Page header)
  %section.content-header
    %h1
      Timeline
      %small Profile timeline
    %ol.breadcrumb
      %li
        %a{:href => "#"}
          %i.fa.fa-dashboard
          Home
      %li
        %a{:href => "#"} UI
      %li.active Timeline
  / Main content
  %section.content
    / row
    .row
      .col-md-12
        / The time line
        %ul.timeline
          / timeline time label
          %li.time-label{"data-aos" => "fade-up"}
            %span.bg-red
              10 Feb. 2014
          / /.timeline-label
          / timeline item
          %li{"data-aos" => "fade-up"}
            %i.fa.fa-envelope.bg-blue
            .timeline-item
              %span.time
                %i.fa.fa-clock-o
                12:05
              %h3.timeline-header
                %a{:href => "#"} Support Team
                sent you an email
              .timeline-body
                Etsy doostang zoodles disqus groupon greplin oooj voxy zoodles,
                weebly ning heekya handango imeem plugg dopplr jibjab, movity
                jajah plickers sifteo edmodo ifttt zimbra. Babblely odeo kaboodle
                quora plaxo ideeli hulu weebly balihoo...
              .timeline-footer
                %a.btn.btn-primary.btn-xs Read more
                %a.btn.btn-danger.btn-xs Delete
          / END timeline item
          / timeline item
          %li{"data-aos" => "fade-up"}
            %i.fa.fa-user.bg-aqua
            .timeline-item
              %span.time
                %i.fa.fa-clock-o
                5 mins ago
              %h3.timeline-header.no-border
                %a{:href => "#"} Madison Olson
                accepted your friend request
          / END timeline item
          / timeline item
          %li{"data-aos" => "fade-up"}
            %i.fa.fa-comments.bg-yellow
            .timeline-item
              %span.time
                %i.fa.fa-clock-o
                27 mins ago
              %h3.timeline-header
                %a{:href => "#"} Jay White
                commented on your post
              .timeline-body
                Take me to your leader!
                Switzerland is small and neutral!
                We are more like Germany, ambitious and misunderstood!
              .timeline-footer
                %a.btn.btn-warning.btn-flat.btn-xs View comment
          / END timeline item
          / timeline time label
          %li.time-label{"data-aos" => "fade-up"}
            %span.bg-green
              3 Jan. 2014
          / /.timeline-label
          / timeline item
          %li{"data-aos" => "fade-up"}
            %i.fa.fa-camera.bg-purple
            .timeline-item
              %span.time
                %i.fa.fa-clock-o
                2 days ago
              %h3.timeline-header
                %a{:href => "#"} Mina Lee
                uploaded new photos
              .timeline-body
                %img.margin{:alt => "...", :src => "http://placehold.it/150x100"}/
                %img.margin{:alt => "...", :src => "http://placehold.it/150x100"}/
                %img.margin{:alt => "...", :src => "http://placehold.it/150x100"}/
                %img.margin{:alt => "...", :src => "http://placehold.it/150x100"}/
          / END timeline item
          / timeline item
          %li{"data-aos" => "fade-up"}
            %i.fa.fa-video-camera.bg-maroon
            .timeline-item
              %span.time
                %i.fa.fa-clock-o
                5 days ago
              %h3.timeline-header
                %a{:href => "#"} Mr. Doe
                shared a video
              .timeline-body
                .embed-responsive.embed-responsive-16by9
                  %iframe.embed-responsive-item{:allowfullscreen => "", :frameborder => "0", :src => "https://www.youtube.com/embed/tMWkeBIohBs"}
              .timeline-footer
                %a.btn.btn-xs.bg-maroon{:href => "#"} See comments
          / END timeline item
          %li{"data-aos" => "fade-up"}
            %i.fa.fa-clock-o.bg-gray
      / /.col
    / /.row
  / /.content
/ /.content-wrapper
