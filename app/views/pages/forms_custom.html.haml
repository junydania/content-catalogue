- content_for :before_scripts do
  %link{:href => "/plugins/daterangepicker/daterangepicker-bs3.css", :rel => "stylesheet"}
    %link{:href => "/plugins/iCheck/all.css", :rel => "stylesheet"}
      %link{:href => "/plugins/colorpicker/bootstrap-colorpicker.min.css", :rel => "stylesheet"}
        %link{:href => "/plugins/timepicker/bootstrap-timepicker.min.css", :rel => "stylesheet"}
          %link{:href => "/plugins/select2/select2.min.css", :rel => "stylesheet"}
          .content-wrapper.forms-custom-page
            / Content Header (Page header)
            %section.content-header
              %h1
                Advanced Form Elements
                %small Custom Form Components
              %ol.breadcrumb
                %li
                  %a{:href => "#"}
                    %i.fa.fa-dashboard
                    Home
                %li
                  %a{:href => "#"} Forms
                %li.active Advanced Elements
            / Main content
            %section.content
              / SELECT2 EXAMPLE
              .box.box-default
                .box-header.with-border
                  %h3.box-title Select2
                  .box-tools.pull-right
                    %button.btn.btn-box-tool{"data-widget" => "collapse"}
                      %i.fa.fa-minus
                    %button.btn.btn-box-tool{"data-widget" => "remove"}
                      %i.fa.fa-remove
                / /.box-header
                .box-body
                  .row
                    .col-md-6
                      .form-group
                        %label Minimal
                        %select.form-control.select2{:style => "width: 100%;"}
                          %option{:selected => "selected"} Alabama
                          %option Alaska
                          %option California
                          %option Delaware
                          %option Tennessee
                          %option Texas
                          %option Washington
                      / /.form-group
                      .form-group
                        %label Disabled
                        %select.form-control.select2{:disabled => "disabled", :style => "width: 100%;"}
                          %option{:selected => "selected"} Alabama
                          %option Alaska
                          %option California
                          %option Delaware
                          %option Tennessee
                          %option Texas
                          %option Washington
                      / /.form-group
                    / /.col
                    .col-md-6
                      .form-group
                        %label Multiple
                        %select.form-control.select2{"data-placeholder" => "Select a State", :multiple => "multiple", :style => "width: 100%;"}
                          %option Alabama
                          %option Alaska
                          %option California
                          %option Delaware
                          %option Tennessee
                          %option Texas
                          %option Washington
                      / /.form-group
                      .form-group
                        %label Disabled Result
                        %select.form-control.select2{:style => "width: 100%;"}
                          %option{:selected => "selected"} Alabama
                          %option Alaska
                          %option{:disabled => "disabled"} California (disabled)
                          %option Delaware
                          %option Tennessee
                          %option Texas
                          %option Washington
                      / /.form-group
                    / /.col
                  / /.row
                / /.box-body
                .box-footer
                  Visit
                  %a{:href => "https://select2.github.io/"} Select2 documentation
                  for more examples and information about the plugin.
              / /.box
              .row
                .col-md-6
                  .box.box-danger
                    .box-header
                      %h3.box-title Input masks
                    .box-body
                      / Date dd/mm/yyyy
                      .form-group
                        %label Date masks:
                        .input-group
                          .input-group-addon
                            %i.fa.fa-calendar
                          %input.form-control{"data-inputmask" => "'alias': 'dd/mm/yyyy'", :type => "text"}
                          / /.input group
                        / /.form group
                        / Date mm/dd/yyyy
                        .form-group
                          .input-group
                            .input-group-addon
                              %i.fa.fa-calendar
                            %input.form-control{"data-inputmask" => "'alias': 'mm/dd/yyyy'", :type => "text"}
                            / /.input group
                          / /.form group
                          / phone mask
                          .form-group
                            %label US phone mask:
                            .input-group
                              .input-group-addon
                                %i.fa.fa-phone
                              %input.form-control{"data-inputmask" => "\"mask\": \"(999) 999-9999\"", :type => "text"}
                              / /.input group
                            / /.form group
                            / phone mask
                            .form-group
                              %label Intl US phone mask:
                              .input-group
                                .input-group-addon
                                  %i.fa.fa-phone
                                %input.form-control{"data-inputmask" => "'mask': ['999-999-9999 [x99999]', '+099 99 99 9999[9]-9999']", :type => "text"}
                                / /.input group
                              / /.form group
                              / IP mask
                              .form-group
                                %label IP mask:
                                .input-group
                                  .input-group-addon
                                    %i.fa.fa-laptop
                                  %input.form-control{"data-inputmask" => "'alias': 'ip'", :type => "text"}
                                  / /.input group
                                / /.form group
                              / /.box-body
                            / /.box
                            .box.box-info{"data-aos" => "fade-up"}
                              .box-header
                                %h3.box-title Color  Time Picker
                              .box-body
                                / Color Picker
                                .form-group
                                  %label Color picker:
                                  %input.form-control.my-colorpicker1{:type => "text"}
                                  / /.form group
                                  / Color Picker
                                  .form-group
                                    %label Color picker with addon:
                                    .input-group.my-colorpicker2
                                      %input.form-control{:type => "text"}
                                        .input-group-addon
                                          %i
                                      / /.input group
                                    / /.form group
                                    / time Picker
                                    .bootstrap-timepicker
                                      .form-group
                                        %label Time picker:
                                        .input-group
                                          %input.form-control.timepicker{:type => "text"}
                                            .input-group-addon
                                              %i.fa.fa-clock-o
                                          / /.input group
                                        / /.form group
                                    / /.box-body
                                  / /.box
                                / /.col (left)
                                .col-md-6
                                  .box.box-primary
                                    .box-header
                                      %h3.box-title Date picker
                                    .box-body
                                      / Date range
                                      .form-group
                                        %label Date range:
                                        .input-group
                                          .input-group-addon
                                            %i.fa.fa-calendar
                                          %input#reservation.form-control.pull-right{:type => "text"}
                                          / /.input group
                                        / /.form group
                                        / Date and time range
                                        .form-group
                                          %label Date and time range:
                                          .input-group
                                            .input-group-addon
                                              %i.fa.fa-clock-o
                                            %input#reservationtime.form-control.pull-right{:type => "text"}
                                            / /.input group
                                          / /.form group
                                          / Date and time range
                                          .form-group
                                            %label Date range button:
                                            .input-group
                                              %button#daterange-btn.btn.btn-default.pull-right
                                                %i.fa.fa-calendar
                                                Date range picker
                                                %i.fa.fa-caret-down
                                          / /.form group
                                        / /.box-body
                                      / /.box
                                      / iCheck
                                      .box.box-success{"data-aos" => "fade-up"}
                                        .box-header
                                          %h3.box-title iCheck - Checkbox  Radio Inputs
                                        .box-body
                                          / Minimal style
                                          / checkbox
                                          .form-group
                                            %label
                                              %input.minimal{:type => "checkbox"}
                                              %label
                                                %input.minimal{:type => "checkbox"}
                                                %label
                                                  %input.minimal{:type => "checkbox"}
                                                    Minimal skin checkbox
                                                / radio
                                                .form-group
                                                  %label
                                                    %input.minimal{:name => "r1", :type => "radio"}
                                                    %label
                                                      %input.minimal{:name => "r1", :type => "radio"}
                                                      %label
                                                        %input.minimal{:name => "r1", :type => "radio"}
                                                          Minimal skin radio
                                                      / Minimal red style
                                                      / checkbox
                                                      .form-group
                                                        %label
                                                          %input.minimal-red{:type => "checkbox"}
                                                          %label
                                                            %input.minimal-red{:type => "checkbox"}
                                                            %label
                                                              %input.minimal-red{:type => "checkbox"}
                                                                Minimal red skin checkbox
                                                            / radio
                                                            .form-group
                                                              %label
                                                                %input.minimal-red{:name => "r2", :type => "radio"}
                                                                %label
                                                                  %input.minimal-red{:name => "r2", :type => "radio"}
                                                                  %label
                                                                    %input.minimal-red{:name => "r2", :type => "radio"}
                                                                      Minimal red skin radio
                                                                  / Minimal red style
                                                                  / checkbox
                                                                  .form-group
                                                                    %label
                                                                      %input.flat-red{:type => "checkbox"}
                                                                      %label
                                                                        %input.flat-red{:type => "checkbox"}
                                                                        %label
                                                                          %input.flat-red{:type => "checkbox"}
                                                                            Flat green skin checkbox
                                                                        / radio
                                                                        .form-group
                                                                          %label
                                                                            %input.flat-red{:name => "r3", :type => "radio"}
                                                                            %label
                                                                              %input.flat-red{:name => "r3", :type => "radio"}
                                                                              %label
                                                                                %input.flat-red{:name => "r3", :type => "radio"}
                                                                                  Flat green skin radio
                                                                            / /.box-body
                                                                            .box-footer
                                                                              Many more skins available.
                                                                              %a{:href => "http://fronteed.com/iCheck/"} Documentation
                                                                          / /.box
                                                                        / /.col (right)
                                                                      / /.row
                                                                    / /.content
                                                                  / /.content-wrapper
                                                                  - content_for :after_body do
                                                                    %script{:src => "/plugins/select2/select2.full.min.js"}
                                                                    %script{:src => "/plugins/input-mask/jquery.inputmask.js"}
                                                                    %script{:src => "/plugins/input-mask/jquery.inputmask.date.extensions.js"}
                                                                    %script{:src => "/plugins/input-mask/jquery.inputmask.extensions.js"}
                                                                    %script{:src => "/plugins/moment/moment.js"}
                                                                    %script{:src => "/plugins/daterangepicker/daterangepicker.js"}
                                                                    %script{:src => "/plugins/colorpicker/bootstrap-colorpicker.min.js"}
                                                                    %script{:src => "/plugins/timepicker/bootstrap-timepicker.min.js"}
                                                                    %script{:src => "/plugins/iCheck/icheck.min.js"}
                                                                    %script{:src => "/assets/pages/forms-custom.js"}
