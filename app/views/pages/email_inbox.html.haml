            %h3.box-title Folders
            .box-tools
              %button.btn.btn-box-tool{"data-widget" => "collapse"}
                %i.fa.fa-minus
          .box-body.no-padding
            %ul.nav.nav-pills.nav-stacked
              %li.active
                %a{:href => "#"}
                  %i.fa.fa-inbox
                  Inbox
                  %span.label.label-primary.pull-right 12
              %li
                %a{:href => "#"}
                  %i.fa.fa-envelope-o
                  Sent
              %li
                %a{:href => "#"}
                  %i.fa.fa-file-text-o
                  Drafts
              %li
                %a{:href => "#"}
                  %i.fa.fa-filter
                  Junk
                  %span.label.label-warning.pull-right 65
              %li
                %a{:href => "#"}
                  %i.fa.fa-trash-o
                  Trash
          / /.box-body
        / /. box
